package lanzadorProcesos;


public class LanzadorProcesos {
	public void ejecutar(String ruta) {

		ProcessBuilder pb;
		try {
			pb = new ProcessBuilder(ruta);
			pb.start();
			Process p;
			p = pb.start();
			p.getInputStream();
			p.getOutputStream();

		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
