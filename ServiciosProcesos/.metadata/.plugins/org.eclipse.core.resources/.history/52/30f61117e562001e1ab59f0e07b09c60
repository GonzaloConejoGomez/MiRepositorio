package lanzadorProcesos;


public class LanzadorProcesos {
	public void ejecutar(String ruta) {

		ProcessBuilder pb;
		try {
			pb = new ProcessBuilder(ruta);
			pb.start();
			Process p = new ProcessBuilder("CMD","/C","start","dir").start();
			
			

		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
