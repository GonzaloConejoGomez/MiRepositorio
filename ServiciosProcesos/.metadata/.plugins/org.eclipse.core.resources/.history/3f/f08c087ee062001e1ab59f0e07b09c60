package lanzadorProcesos;


public class LanzadorProcesos {
	public void ejecutar() {

		
		try {
			ProcessBuilder pb = new ProcessBuilder("cmd", "/c", "dir");
            pb.redirectError();
            Process p = pb.start();
            p.getInputStream();
            
            int exitCode = p.waitFor();

            
            System.out.println("Process terminated with " + exitCode);
		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
